{
  "version": 2,
  "builds": [
    {
      "src": "api/moderate.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/moderate",
      "dest": "api/moderate.py"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      }
    }
  ]
}