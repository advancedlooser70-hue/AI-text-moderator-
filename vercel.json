{
  "version": 2,
  "builds": [
    {
      "src": "api/moderate.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.12",
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/moderate",
      "dest": "api/moderate.py"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      }
    }
  ],
  "functions": {
    "api/moderate.py": {
      "maxDuration": 30,
      "memory": 1024
    }
  }
}